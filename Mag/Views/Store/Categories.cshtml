@model List<Categories>
@using Mag.Helpers

@{
    ViewData["Title"] = "Categories";
}
<div class="container">
    <div class="row">
        @foreach(var cat in Model)
        {
            <div class="col-md-3">
                <a asp-controller="Store" asp-action="Products" asp-route-id="@((int)cat)">@cat.GetCategoryName()</a>
            </div>
        }
    </div>
</div>